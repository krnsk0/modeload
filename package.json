{
  "name": "modeload",
  "version": "0.1.3",
  "description": "A simple CLI tool to save and load Cursor custom modes",
  "main": "dist/index.js",
  "homepage": "https://github.com/krnsk0/modeload",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krnsk0/modeload.git"
  },
  "bin": {
    "modeload": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsup && chmod +x dist/index.js",
    "link": "npm link",
    "build-link": "npm run build && npm link",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "cursor",
    "modes",
    "cli",
    "backup"
  ],
  "author": "krnsk0",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "9.6.0",
    "zod": "3.25.46"
  },
  "devDependencies": {
    "@types/better-sqlite3": "7.6.8",
    "@types/node": "20.10.0",
    "tsup": "8.0.1",
    "typescript": "5.3.3",
    "vitest": "3.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
